{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "no-shadow": "off",
    "radix": "off",
    "no-restricted-globals": "off",
    "no-restricted-syntax": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "max-len": [
      "error",
      {
        "code": 170
      }
    ],
    "no-use-before-define": "off"
  }
}